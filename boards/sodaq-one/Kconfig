source "$(RIOTBOARD)/common/sodaq/Kconfig"

config BOARD
    default "sodaq-one" if BOARD_SODAQ_ONE

config BOARD_SODAQ_ONE
    bool
    default y
    select CPU_MODEL_SAMD21G18A
    select BOARDS_COMMON_SODAQ
