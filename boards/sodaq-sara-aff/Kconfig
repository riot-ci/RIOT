source "$(RIOTBOARD)/common/sodaq/Kconfig"

config BOARD
    default "sodaq-sara-aff" if BOARD_SODAQ_SARA_AFF

config BOARD_SODAQ_SARA_AFF
    bool
    default y
    select CPU_MODEL_SAMD21J18A
    select BOARDS_COMMON_SODAQ
